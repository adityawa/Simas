@model SimasSD.Models.SysEmployeeTitleModel

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Employee Title</h4>
    <hr />


    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.EmpyTitleID)
            </dt>

            <dd>
                @Html.TextBoxFor(model => model.EmpyTitleID, new {@readonly="readonly" })
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.EmpyTitleName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.EmpyTitleName, new { @readonly = "readonly" })
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Active)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Active)
            </dd>

        </dl>

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
