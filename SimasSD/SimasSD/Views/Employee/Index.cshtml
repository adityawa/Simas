@model IEnumerable<SimasSD.Models.SysEmployeeModel>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmpyID)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.EmpyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpyGender)
