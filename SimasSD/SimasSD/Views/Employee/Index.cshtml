            @Html.DisplayFor(modelItem => item.EmpyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpyGender)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.EmpyBranchID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmpyTitleID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Active)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id= item.EmpyID }) |
          
